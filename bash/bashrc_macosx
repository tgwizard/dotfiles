echo "Initializing for Mac OS X"
if [ -d ~/code/android-sdk-macosx ]; then
	PATH=~/code/android-sdk-macosx/platform-tools:$PATH
fi

source `brew --prefix`/Library/Contributions/brew_bash_completion.sh

